{
    "name": "__MSG_AppName__",
    "description": "__MSG_Description__",
    "version": "0.0.1",
    "default_locale": "zh_CN",
    "permissions": [
        "storage",
        "declarativeContent",
        "activeTab",
        "tabs"
    ],
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "icons": {
        "16": "images/logo16.png",
        "32": "images/logo32.png",
        "48": "images/logo48.png",
        "128": "images/logo128.png"
    },
    "manifest_version": 2
}
