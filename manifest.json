{
    "name": "TinyTab",
    "description": "简洁的新标签页",
    "version": "1.0.1",
    "manifest_version": 2,
    "permissions": [
        "activeTab",
        "tabs"
    ],
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "icons": {
        "16": "images/logo16.png",
        "32": "images/logo32.png",
        "48": "images/logo48.png",
        "128": "images/logo128.png"
    }
}
